<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
  	<package name="validate" namespace="/" extends="struts-default">
  		<action name="validateAction_*"  method="{1}" class="me.gteam.logman.struts2.action.ValidateAction">
  			<result name="login">/login.jsp</result>
  			<result name="success">/jsp/home/home.jsp</result>
  			<result name="showThirdAuthorities">/jsp/home/right.jsp</result>
  		</action>
  	</package>
</struts>	