<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
  	<package name="orders" namespace="/" extends="struts-default">
  		<action name="ordersAction_*"  method="{1}" class="ordersAction">
  			<result name="listAction">jsp/orders/orderMain.jsp</result>
  			<result name="addUI">jsp/orders/addOrder.jsp</result>
  			<result name="updateUI">WEB-INF/jsp/orders/updateOrders.jsp</result>
  			<result type="redirectAction" name="action2action">ordersAction_getAllorders.action</result>
  		</action>
  	</package>
</struts>	